    "resetSwagger": "git checkout -- src/services & git clean -f",
    "swagger": "venus swagger",
    "test-script": "jest --config ./jest.node.config.js",
    "postinstall": "patch-package",
    "deadcode": "max deadcode",
    "webpack": "max webpack",
    "preview": "max preview",
    "checkUpdates": "ncu --reject /@ctc/ --format group",
    "saga": "bash ./scripts/git-scripts/git-install-saga-monitor.sh",
    "uninstall-saga": "bash ./scripts/git-scripts/git-uninstall-saga-monitor.sh",
    "prePush": "bash ./scripts/git-scripts/prePush.sh",
    "find-unused-files": "node scripts/deadCode/findUnusedFiles.js"
